#################################
# Seafile Community Edition 13  #
# Docker: docker-compose-CE.yml #
# Doc: https://manual.seafile.com/13.0/setup/setup_ce_by_docker/
#################################
APP_NAME_CE=seafile-ce

## Images (CE)
SEAFILE_IMAGE=seafileltd/seafile-mc:13.0-latest
SEAFILE_DB_IMAGE=mariadb:10.11
SEAFILE_REDIS_IMAGE=redis

## Volumes
SEAFILE_VOLUME=/opt/seafile-data
SEAFILE_MYSQL_VOLUME=/opt/seafile-mysql/db

#################################
#      Startup parameters       #
#################################
SEAFILE_SERVER_HOSTNAME=seafile.example.com
SEAFILE_SERVER_PROTOCOL=https
TIME_ZONE=Europe/Paris
# Generate: pwgen -s 40 1 or openssl rand -hex 32
JWT_PRIVATE_KEY=

#####################################
# Third-party service configuration #
#####################################

## Database
SEAFILE_MYSQL_DB_HOST=db
SEAFILE_MYSQL_DB_PORT=3306
SEAFILE_MYSQL_DB_USER=seafile
SEAFILE_MYSQL_DB_PASSWORD=
SEAFILE_MYSQL_DB_CCNET_DB_NAME=ccnet_db
SEAFILE_MYSQL_DB_SEAFILE_DB_NAME=seafile_db
SEAFILE_MYSQL_DB_SEAHUB_DB_NAME=seahub_db

## Cache (Redis recommended since Seafile 13)
CACHE_PROVIDER=redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

## Memcached (only if CACHE_PROVIDER=memcached; not in CE compose by default)
# MEMCACHED_HOST=memcached
# MEMCACHED_PORT=11211

######################################
#        Initial variables           #
# (Only valid on first-time startup) #
######################################
INIT_SEAFILE_MYSQL_ROOT_PASSWORD=
INIT_SEAFILE_ADMIN_EMAIL=admin@example.com
INIT_SEAFILE_ADMIN_PASSWORD=

############################################
# Optional                                  #
############################################
SITE_ROOT=/
NON_ROOT=false
SEAFILE_LOG_TO_STDOUT=false
ENABLE_GO_FILESERVER=true

## Notification server (CE: optional, not in default CE compose)
ENABLE_NOTIFICATION_SERVER=false
# NOTIFICATION_SERVER_URL=https://${SEAFILE_SERVER_HOSTNAME}/notification
